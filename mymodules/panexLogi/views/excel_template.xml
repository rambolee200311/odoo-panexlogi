<odoo>
    <!-- Tree View -->
    <record id="view_excel_template_tree" model="ir.ui.view">
        <field name="name">panexlogi.excel.template.tree</field>
        <field name="model">panexlogi.excel.template</field>
        <field name="arch" type="xml">
            <tree string="Excel Templates" editable="bottom">
                <field name="type"/>
                <field name="project" optional="show"/>
                <field name="template_file" widget="binary_filename" filename="template_file_name"/>
                <field name="template_file_name" optional="hide"/>
                <field name="remark" optional="show"/>
            </tree>
        </field>
    </record>

    <!-- Action -->
    <record id="action_excel_template" model="ir.actions.act_window">
        <field name="name">Excel Templates</field>
        <field name="res_model">panexlogi.excel.template</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="view_excel_template_tree"/>
    </record>
</odoo>