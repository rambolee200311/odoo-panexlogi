<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- window_action -->
    <record id="panexlogi_deliverytype_action" model="ir.actions.act_window">
        <field name="name">deliverytype.management</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">panexlogi.deliverytype</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                create your first one!
            </p>
        </field>
    </record>
    <!-- Form View -->
    <record id="deliverytype_view_form" model="ir.ui.view">
        <field name="name">panexlogi.deliverytype.form</field>
        <field name="model">panexlogi.deliverytype</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group string="Main">
                        <field name="deliverytype_shortname"/>
                        <field name="deliverytype_name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!--Search View-->
        <record id="settle_bill_search_view" model="ir.ui.view">
            <field name="name">panexlogi.deliverytype.search</field>
            <field name="model">panexlogi.deliverytype</field>
            <field name="arch" type="xml">
                <search>
                    <field name="deliverytype_shortname"/>
                    <field name="deliverytype_name"/>
                </search>
            </field>
        </record>
    <!--Tree(List) View-->
    <record id="deliverytype_view_tree" model="ir.ui.view">
        <field name="name">deliverytype.tree</field>
        <field name="model">panexlogi.deliverytype</field>
        <field name="arch" type="xml">
            <tree>
                <field name="deliverytype_shortname"/>
                <field name="deliverytype_name"/>
            </tree>
        </field>
    </record>
    <!-- window_action -->
    <record id="panexlogi_trailertype_action" model="ir.actions.act_window">
        <field name="name">trailertype.management</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">panexlogi.trailertype</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                create your first one!
            </p>
        </field>
    </record>
     <!--Tree(List) View-->
    <record id="trailertype_view_tree" model="ir.ui.view">
        <field name="name">trailertype.tree</field>
        <field name="model">panexlogi.trailertype</field>
        <field name="arch" type="xml">
            <tree>
                <field name="trailertype_shortname"/>
                <field name="trailertype_name"/>
            </tree>
        </field>
    </record>
</odoo>