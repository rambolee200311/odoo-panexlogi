<odoo>
    <record id="view_clearance_type_tree" model="ir.ui.view">
        <field name="name">clearance.type.tree</field>
        <field name="model">panexlogi.clearance.type</field>
        <field name="arch" type="xml">
            <tree string="Clearance Types" editable="bottom">
                <field name="code"/>
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>
    <record id="action_clearance_type" model="ir.actions.act_window">
        <field name="name">Clearance Types</field>
        <field name="res_model">panexlogi.clearance.type</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>