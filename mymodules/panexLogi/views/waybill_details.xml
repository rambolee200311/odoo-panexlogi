<odoo>
<!-- Search for arrival notice -->
    <record id="waybill_details_view_search" model="ir.ui.view">
        <field name="name">waybill.details.search.view</field>
        <field name="model">panexlogi.waybill.details</field>
        <field name="arch" type="xml">
            <search>
                <field name="cntrno"/>
                <field name="uncode"/>
                <field name="cntrnum"/>
                <field name="note"/>
<!--                <field name="shipping"/>-->
            </search>
        </field>
    </record>
<!--    arrival notice tree-->
    <record id="panexlogi_waybill_details_tree" model="ir.ui.view">
        <field name="name">panexlogi.waybill.details.tree</field>
        <field name="model">panexlogi.waybill.details</field>
        <field name="arch" type="xml">
            <tree string="Waybill_details_Tree">
                <field name="cntrno"/>
                <field name="uncode"/>
                <field name="pallets"/>
                <field name="cntrnum"/>
                <field name="note"/>
                <field name="truck_type"/>
                <field name="warehouse"/>
               <field name="delivery_address"/>
            </tree>
        </field>
    </record>
<!--    arrival notice form-->
    <record id="panexlogi_waybill_details_form" model="ir.ui.view">
        <field name="name">panexlogi.waybill.details.form</field>
        <field name="model">panexlogi.waybill.details</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            Container Details
                        </h1>
                    </div>
                    <group>
                        <field name="cntrno"/>
                        <field name="uncode"/>
                        <field name="pallets"/>
                        <field name="cntrnum"/>
                        <field name="note"/>
                        <field name="truck_type"/>
                    </group>
                    <notebook>
                        <page string="Inboud">
                            <group col="2">
                                <group col="2" readonly="truck_type != 'inbound'">
                                    <field name="warehouse"/>
                                    <field name="warehouse_address_street"/>
                                    <field name="warehouse_address_city"/>
                                    <field name="warehouse_address_state"/>
                                    <field name="warehouse_address_country"/>
                                </group>
                                <group col="2" readonly="truck_type != 'inbound'">
                                    <field name="warehouse_address_zip"/>
                                    <field name="warehouse_phone"/>
                                    <field name="warehouse_mobile"/>
                                </group>
                            </group>
                        </page>
                        <page string="Delivery">
                            <group col="2">
                                <group col="2" readonly="truck_type != 'delivery'">
                                    <field name="delivery_type"/>
                                    <field name="delivery_address"/>
                                    <field name="delivery_company_name"/>
                                </group>
                                <group col="2" readonly="truck_type != 'delivery'">
                                    <field name="delivery_contact_phone"/>
                                    <field name="delivery_postcode"/>
                                    <field name="delivery_country"/>
                                    <field name="delivery_address_timeslot"/>
                                </group>
                            </group>
                        </page>
                    </notebook>>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" />
                    <field name="activity_ids" widget="mail_activity" />
                    <field name="message_ids" widget="mail_thread" />
                </div>
            </form>
        </field>
    </record>
 <!-- action -->
    <record id="waybill_details_action" model="ir.actions.act_window">
        <field name="name">Container Details</field>
        <field name="res_model">panexlogi.waybill.details</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>