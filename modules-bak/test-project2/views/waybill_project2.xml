<odoo>
<!--        wayBill action -->
    <record id="waybill_project2_action" model="ir.actions.act_window">
        <field name="name">Waybill of project2</field>
        <field name="res_model">panexwd.europe.waybill.project2</field>
        <field name="view_mode">tree,form</field>
    </record>
<!-- Search for waybill -->
    <record id="waybill_project2_view_search" model="ir.ui.view">
        <field name="name">panexwd.europe.waybill.project2.search</field>
        <field name="model">panexwd.europe.waybill.project2</field>
        <field name="arch" type="xml">
            <search>
                <field name="project"/>
                <field name="billno"/>
                <field name="date"/>
                <field name="charDef1" string="a11"/>
                <field name="charDef2" string="a22"/>
                <field name="charDef3" string="a33"/>
                <field name="p2_billno"/>
            </search>
        </field>
    </record>
<!--waybill tree-->
     <record id="waybill_project2_view_tree" model="ir.ui.view">
        <field name="name">panexwd.europe.waybill.project2.tree</field>
        <field name="model">panexwd.europe.waybill.project2</field>
        <field name="arch" type="xml">
            <tree string="Seawaybill_Tree">
                <field name="project"/>
                <field name="billno"/>
                <field name="date"/>
                <field name="charDef1" string="a11"/>
                <field name="charDef2" string="a22"/>
                <field name="charDef3" string="a33"/>
                <field name="p2_billno"/>
            </tree>
        </field>
     </record>
<!--waybill form-->
     <record id="waybill_project2_view_form" model="ir.ui.view">
        <field name="name">panexwd.europe.waybill.project2.form</field>
        <field name="model">panexwd.europe.waybill.project2</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            Waybill
                        </h1>
                        <h2>
                            <field name="billno"/>
                        </h2>
                        <h2>
                            <field name="p2_billno"/>
                        </h2>
                    </div>
                    <group col="2">
                        <group string="General">
                            <field name="project"/>
                            <field name="date"/>
                            <field name="p2_charDef1"/>
                        </group>

                     </group>
                    <notebook>
                        <page  string="Others">
                            <sheet>
                                <group>
                                    <field name="charDef1" string="a11"/>
                                    <field name="charDef2" string="a22"/>
                                    <field name="charDef3" string="a33"/>
                                </group>
                            </sheet>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" />
                    <field name="activity_ids" widget="mail_activity" />
                    <field name="message_ids" widget="mail_thread" />
                </div>
            </form>
        </field>
     </record>
</odoo>