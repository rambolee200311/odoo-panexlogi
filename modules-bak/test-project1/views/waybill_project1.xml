<odoo>
<!--        wayBill action -->
    <record id="waybill_project1_action" model="ir.actions.act_window">
        <field name="name">Waybill of Project1</field>
        <field name="res_model">panexwd.europe.waybill.project1</field>
        <field name="view_mode">tree,form</field>
    </record>
<!-- Search for waybill -->
    <record id="waybill_project1_view_search" model="ir.ui.view">
        <field name="name">panexwd.europe.waybill.project1.search</field>
        <field name="model">panexwd.europe.waybill.project1</field>
        <field name="arch" type="xml">
            <search>
                <field name="project"/>
                <field name="billno"/>
                <field name="date"/>
                <field name="charDef1" string="111"/>
                <field name="charDef2" string="222"/>
                <field name="charDef3" string="333"/>
                <field name="p1_billno"/>
            </search>
        </field>
    </record>
<!--waybill tree-->
     <record id="waybill_project1_view_tree" model="ir.ui.view">
        <field name="name">panexwd.europe.waybill.project1.tree</field>
        <field name="model">panexwd.europe.waybill.project1</field>
        <field name="arch" type="xml">
            <tree string="Seawaybill_Tree">
                <field name="project"/>
                <field name="billno"/>
                <field name="date"/>
                <field name="charDef1" string="111"/>
                <field name="charDef2" string="222"/>
                <field name="charDef3" string="333"/>
                <field name="p1_billno"/>
            </tree>
        </field>
     </record>
<!--waybill form-->
     <record id="waybill_project1_view_form" model="ir.ui.view">
        <field name="name">panexwd.europe.waybill.project1.form</field>
        <field name="model">panexwd.europe.waybill.project1</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            Waybill
                        </h1>
                        <h2>
                            <field name="billno"/>
                        </h2>
                        <h2>
                            <field name="p1_billno"/>
                        </h2>
                    </div>
                    <group col="2">
                        <group string="General">
                            <field name="project"/>
                            <field name="date"/>
                        </group>

                     </group>
                    <notebook>
                        <page  string="Others">
                            <sheet>
                                <group>
                                    <field name="charDef1" string="111"/>
                                    <field name="charDef2" string="222"/>
                                    <field name="charDef3" string="333"/>
                                </group>
                            </sheet>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
     </record>
</odoo>